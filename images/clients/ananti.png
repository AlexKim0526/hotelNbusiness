                $('#agreeChk3').prop('checked',true);
                $('#agreeChk4').prop('checked',true);
            }else{
                $('#agreeChk1').prop('checked',false);
                $('#agreeChk2').prop('checked',false);
                $('#agreeChk3').prop('checked',false);
                $('#agreeChk4').prop('checked',false);
            }
        });

