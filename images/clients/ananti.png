        });

        /*약관동의 확인*/
        $('#agreeOkBtn').on("click",function(){
            if(!$('#agreeChk1').prop('checked')){
                // alertLayer("회원약관 동의는 필수입니다.");
                alert("회원약관 동의는 필수입니다.");
