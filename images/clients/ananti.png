                return ;
            }
            if(!$('#agreeChk3').prop('checked')){
                // alertLayer("개인정보 제 3자 제공 동의는 필수입니다.");
                alert("개인정보 제 3자 제공 동의는 필수입니다.");
                return ;
            }

            // cmNo
            let params = {
                cmNo: cmNo,
                agreeChk4: $('#agreeChk4').prop('checked')?"Y":"N",
            }
            $.ajax({
                url: "/ko/agreement/save",
